{
    "name": "apps",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "antd": "^3.5.3",
        "classnames": "^2.2.5",
        "react": "^16.3.2",
        "react-dom": "^16.3.2",
        "react-redux": "^5.0.7",
        "react-router-dom": "^4.2.2",
        "redux": "^4.0.0",
        "reselect": "^3.0.1",
        "typesafe-actions": "^2.0.3",
        "vis": "^4.21.0"
    },
    "devDependencies": {
        "@types/classnames": "^2.2.3",
        "@types/react": "^16.3.14",
        "@types/react-dom": "^16.0.5",
        "@types/react-redux": "^6.0.0",
        "@types/react-router-dom": "^4.2.6",
        "@types/vis": "^4.21.4",
        "awesome-typescript-loader": "^5.0.0",
        "create-react-app": "^1.5.2",
        "css-loader": "^0.28.11",
        "file-loader": "^1.1.11",
        "html-webpack-plugin": "^3.2.0",
        "mini-css-extract-plugin": "^0.4.0",
        "npm": "^6.0.1",
        "react-hot-loader": "^4.2.0",
        "redux-devtools-extension": "^2.13.2",
        "source-map-explorer": "^1.5.0",
        "style-loader": "^0.21.0",
        "tslint-react": "^3.6.0",
        "typescript": "^2.8.3",
        "webpack": "^4.8.3",
        "webpack-cli": "^2.1.3",
        "webpack-dashboard": "^1.1.1",
        "webpack-dev-server": "^3.1.4",
        "webpack-serve": "^1.0.2",
        "yarn": "^1.6.0"
    },
    "scripts": {
        "start": "react-scripts-ts start",
        "build": "webpack --config configs/webpack.config.js",
        "clean": "rimraf dist",
        "build2": "webpack-dashboard -- webpack --config configs/webpack.config.js --env.build --env.sourceMap",
        "dev": "webpack-dashboard -m -- webpack-dev-server --config configs/webpack.config.js --open --env.awesome",
        "dev:dashboard": "webpack-dashboard -m -- webpack-dev-server --config configs/webpack.config.js --open",
        "lint": "tslint --project tsconfig.json",
        "serve": "webpack-serve --config configs/webpack.config.js",
        "tsc": "tsc -p . --noEmit",
        "tsc:watch": "tsc -p . --noEmit -w",
        "test:watch": "jest --config jest.json --watch",
        "test:update": "jest --config jest.json -u",
        "reinstall": "rm -rf node_modules && yarn",
        "reinstall:win": "rd /s /q node_modules && yarn",
        "graph": "tsc && madge --image graph.svg out/index.js --warning",
        "analyze": "source-map-explorer dist/static/bundle.js"
    }
}
