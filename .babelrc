{
    "presets": ["@babel/react", "@babel/preset-stage-1"],

    "env": {
        "test": {
            "plugins": ["transform-es2015-modules-commonjs"]
        }
    },

    "plugins": [
        "@babel/proposal-object-rest-spread",
        ["module-resolver", {
            "root": ["./src"],
            "alias": { "@": "./src" }
        }]
    ]
}