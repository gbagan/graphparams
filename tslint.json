{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:latest",
        "tslint-react"
    ],
    "rules": {
        "arrow-parens": false,
        "curly": false,
        "array-type": false,
        "interface-over-type-literal": false,
        "no-bitwise": false,
        "no-submodule-imports": false,
        "jsx-boolean-value": false,
        "jsx-alignment": false,
        "jsx-no-multiline-js": false
    }
}