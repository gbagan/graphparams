{
  "compilerOptions": {
    "target": "esnext",
    "jsx": "react",
    "module": "esnext",
    "removeComments": false,
    "strict": true,
    "outDir": "./out",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "noUnusedLocals": true,
    //"noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
                                    
},
  "exclude": [
    "node_modules"
  ]
}